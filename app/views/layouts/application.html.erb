<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <meta name="viewport" content="initial-scale = 1.0,maximum-scale = 1.0" />
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js">
  </script>
  <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-43170225-1', 'paseoseattle.com');
  ga('send', 'pageview');

</script>
  <link rel="shortcut icon" type="image/png" href="assets/favicon3.png" />
</head>
<body>
      <%= render 'layouts/header' %>
      <div class="row" id="backdrop">     
        <div class="col-sm-12 col-md-8 col-lg-8 text-box center" id="main-text"> 
          <%= yield %>
          <div class="container">
          <%= render 'layouts/connect' %>
          </div>
        </div>  
      </div>    
  
    <%= render 'layouts/footer' %>

  <script>
  $(document).ready(function () {
        $('ul.nav > li').click(function (e) {
            $('ul.nav > li').removeClass('active');
            $(this).addClass('active');                
        });            
    });
  </script>
</body>
</html>